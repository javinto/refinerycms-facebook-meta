<%= raw %(<meta property="og:title" content="#{@meta.og_title}" />) if @meta.og_title.present? %>
<%= raw %(<meta property="og:description" content="#{@meta.og_description}" />) if @meta.og_description.present? %>
<%= raw %(<meta property="og:image" content="#{@meta.og_image.url(host: request.protocol + request.host_with_port)}" />) if @meta.og_image.present? %>